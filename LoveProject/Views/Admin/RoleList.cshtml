@model Microsoft.EntityFrameworkCore.Internal.InternalDbSet<IdentityRole>
<h1> Role List</h1>
@if (Model.Count() > 0)
{
    @foreach (var role in Model)
    {
        <div>
            <p>@role.Name</p>
            <form asp-action="RoleDelete" asp-controller="Admin" method="post">
                <input type="hidden" value="@role.Name" name="Name"/>
                <button type="submit">Delete</button>
            </form>
            <form asp-action="RoleEdit" asp-controller="Admin" method="get">
                <input type="hidden" value="@role.Name" name="Name"/>
                <button type="submit">Edit</button>
            </form>

        </div>

    }

}
else
{
    
    <h5>No Role Yet</h5>  
}
