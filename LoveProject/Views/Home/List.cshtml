@model List<LoverMatch>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Love List</title>
</head>
<body>
    
    <form action="/Home/List">
        <input type="text" placeholder="SearchBar" name="q"/>
        <button type="submit">Search</button>
    </form>
    <hr />
    
    @foreach (var lover in Model)
    {
        <div>
            <p>@lover.Name1</p>
            <p>@lover.Name2</p>
            <p>@lover.Score</p>
            <form action="/Home/Delete" method="POST">
            <input type="hidden" value="@lover.Id" name="Id">
            <button type="submit">Delete</button>    
            </form>
        </div>
    }

</body>
</html>